1-SELECT nombre, apellido,fecha_nacimiento FROM profesor p
PRDER BY p.fecha_nacimiento ASC
2-SELECT nombre, apellido FROM profesor 
WHERE salario >= 65000
3-SELECT nombre,apellido FROM profesor
WHERE fecha_nacimiento BETWEEN '01-01-1980' and '31-12-1989'
4-SELECT TOP 5 * FROM profesor;
5- SELECT nombre, apellido FROM profesor WHERE substring(apellido,1,1)='P'
6- -SELECT nombre,apellido FROM profesor
WHERE fecha_nacimiento BETWEEN '01-01-1980' and '31-12-1989' AND salario > 80000
